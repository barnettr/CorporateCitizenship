				<DiscreteObjectKeyFrame KeyTime="0:0:0.15" Value="Collapsed" />
			</ObjectAnimationUsingKeyFrames>
			<DoubleAnimation Storyboard.TargetName="Headline" Storyboard.TargetProperty="(UIElement.Opacity)" To="0" Duration="0:0:.1" />
